To clear database:
DELETE FROM advertisers;
DELETE FROM adv_story;
DELETE FROM announcements;
DELETE FROM clicks;
DELETE FROM history;
DELETE FROM notifications;
DELETE FROM payments;
DELETE FROM payment_requests;
DELETE FROM publishers;
DELETE FROM publishers_websites;
DELETE FROM pub_story;
DELETE FROM views;
DELETE FROM withdrawal;

set global sql_mode='STRICT_TRANS_TABLES,NO_ZERO_IN_DATE,NO_ZERO_DATE,ERROR_FOR_DIVISION_BY_ZERO,NO_AUTO_CREATE_USER,NO_ENGINE_SUBSTITUTION';
set session sql_mode='STRICT_TRANS_TABLES,NO_ZERO_IN_DATE,NO_ZERO_DATE,ERROR_FOR_DIVISION_BY_ZERO,NO_AUTO_CREATE_USER,NO_ENGINE_SUBSTITUTION';


CREATE USER 'phpmyadmin_user'@'localhost' IDENTIFIED BY 'pLA%s56YR4*%ao';
GRANT ALL PRIVILEGES ON *.* TO 'phpmyadmin_user'@'localhost' WITH GRANT OPTION;
FLUSH PRIVILEGES;